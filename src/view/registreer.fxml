<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<StackPane prefHeight="149.0" prefWidth="267.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegistreerController">
  <children>
    <StackPane>
      <children>
        <VBox spacing="10.0">
          <children>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
                     <children>
                        <TextField fx:id="txtFieldNaam" promptText="naam" GridPane.columnIndex="1" />
                        <TextField fx:id="txtFieldCity" promptText="woonplaats" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <PasswordField fx:id="txtFieldPassword" promptText="password" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                     </children>
            </GridPane>
            <StackPane prefHeight="22.0" prefWidth="247.0">
                     <children>
                        <Button fx:id="btnRegistreren" mnemonicParsing="false" onAction="#RegistrerenAction" text="registreer" StackPane.alignment="CENTER_RIGHT" />
                        <Button fx:id="btnGoBack" mnemonicParsing="false" onAction="#goBackToLogin" text="Go Back" />
                     </children>
            </StackPane>
          </children>
        </VBox>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>
